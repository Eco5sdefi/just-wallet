{
  "name": "ethers-wallet-js",
  "version": "0.0.1",
  "description": "ERC Wallet framework for Eth",
  "main": "./dist/require.js",
  "module": "./es6/Wallet.js",
  "scripts": {
    "compile": "tsc --watch",
    "build": "tsc -p tsconfig.json && tsc -p  es6tsconfig.json",
    "test": "ts-jest",
    "jest" : "jest --testTimeout=25000",
    "jest-from-signer" : "jest --testTimeout=20000 -- FromSigner.test.js",
    "jest-wallet" : "jest --testTimeout=20000 -- Wallet.test.js",
    "jest-wallet-from-mnemonic" : "jest --testTimeout=20000 -- WalletFromMnemonic.test.js",
    "jest-format" : "jest --testTimeout=20000 -- Format.test.js",
    "jest-valid-address" : "jest --testTimeout=20000 -- isValidAddress.test.js",
    "jest-wallet-address" : "jest --testTimeout=20000 -- Wallet-address.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lowecainandaya/eth-wallet.git"
  },
  "keywords": [
    "erc20",
    "wallet",
    "crypto",
    "eth",
    "bitcoin"
  ],
  "author": "Lowe Andaya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lowecainandaya/eth-wallet/issues"
  },
  "homepage": "https://github.com/lowecainandaya/eth-wallet#readme",
  "dependencies": {
    "@validitor/main": "^0.0.9",
    "ethers": "^5.4.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
